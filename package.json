{
  "name": "react-webpack-practice",
  "version": "1.0.0",
  "description": "WebpackにてReact環境を構築",
  "main": "index.js",
  "scripts": {
    "clean": "rm -fR ./dist",
    "build": "npm run clean && mkdir ./dist && cp ./index.html ./dist/ && webpack --mode production",
    "build-dev": "npm run clean && mkdir ./dist && cp ./index.html ./dist/ && webpack --mode development",
    "start": "serve ./dist"
